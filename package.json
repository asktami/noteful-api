{
	"name": "noteful-api",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test": "mocha --require test/setup.js",
		"dev": "nodemon src/server.js",
		"stage": "env DATABASE_URL=$(heroku config:get DATABASE_URL)?ssl=true nodemon src/server.js",
		"migrate": "postgrator --config postgrator-config.js",
		"migrate:test": "env NODE_ENV=test npm run migrate",
		"migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
		"start": "node src/server.js",
		"predeploy": "npm audit && npm run migrate:production",
		"deploy": "git push heroku master"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^8.1.0",
		"express": "^4.17.1",
		"helmet": "^3.21.1",
		"knex": "^2.4.0",
		"morgan": "^1.9.1",
		"node": "^10.24.0",
		"pg": "^7.18.2",
		"uuid": "^3.3.3",
		"winston": "^3.2.1",
		"xss": "^1.0.6"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"eslint": "^6.5.1",
		"eslint-plugin-prettier": "^3.1.1",
		"mocha": "^7.2.0",
		"nodemon": "^2.0.4",
		"postgrator-cli": "^3.2.0",
		"prettier": "1.18.2",
		"supertest": "^4.0.2"
	},
	"engines": {
		"node": "10.x"
	}
}
